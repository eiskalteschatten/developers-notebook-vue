{
  "name": "developers-notebook",
  "version": "0.1.0",
  "description": "A digital notebook for developers",
  "main": "./src/server.js",
  "private": true,
  "scripts": {
    "test": "npm run lint",
    "lint": "./node_modules/.bin/eslint 'electron/src/**/*.{js,vue}' 'frontend/src/**/*.{js,vue}' 'server/src/**/*.js'",
    "install-all": "cd electron && npm ci && cd ../frontend && npm ci && cd ../server npm ci"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:eiskalteschatten/developers-notebook.git"
  },
  "author": "Alex Seifert",
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
